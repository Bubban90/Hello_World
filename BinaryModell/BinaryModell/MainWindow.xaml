<Window x:Class="BinaryModell.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:BinaryModell"
        Title="MainWindow" Height="612" Width="1024">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100*"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="100*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <local:testWindow x:Name="localTest" Visibility="Hidden"></local:testWindow>
        <local:examWindow x:Name="localExam" Visibility="Hidden"></local:examWindow>
        <local:menu x:Name="localName" Visibility="Visible"></local:menu>

    </Grid>    
</Window>
